type video {
  id: ID! @unique
  userId: String!
  title: String!
  slug: String!
  description: String!
  video: String!
  thumbnail: String!
  state: String!
  category: String!
  views: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type label {
  id: ID! @unique
  name: String!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type user {
  id: ID! @unique
  username: String!
  email: String!
  password: String!
  followers: Int!
  bio: String!
  image: String!
  createdAt: DateTime!
  updatedAt: DateTime!
}